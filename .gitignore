localexplorer-backend/build/
localexplorer-backend/.idea/         # IntelliJ IDEA
localexplorer-backend/.vscode/       # VS Code
localexplorer-backend/*.iml          # IntelliJ IDEA
localexplorer-backend/target/        # Maven build output
localexplorer-backend/src/*/resources/*.log
localexplorer-backend/src/*/resources/*.sql
